void createSensorJson(char *tag, float value, char *unit) {  
  jsonDocument.clear();  
  jsonDocument["type"] = tag;
  jsonDocument["value"] = value;
  jsonDocument["unit"] = unit;
  serializeJson(jsonDocument, buffer);
}
 
void addJsonObject(char *tag, float value, char *unit) {
  JsonObject obj = jsonDocument.createNestedObject();
  obj["type"] = tag;
  obj["value"] = value;
  obj["unit"] = unit; 
}


void connectToWiFi() {
    Serial.println("\nConnecting to WiFi...");

    WiFi.mode(WIFI_STA); // Optional
    WiFi.begin(ssid, password);

    int timeout_counter = 0;
    while (WiFi.status() != WL_CONNECTED) {
        Serial.print(".");
        delay(200);
        timeout_counter++;
        if (timeout_counter >= CONNECTION_TIMEOUT * 5) {
            Serial.println("\nFailed to connect to WiFi. Restarting...");
            ESP.restart();
        }
    }

    Serial.println("\nConnected to the WiFi network");
    Serial.print("Local ESP32 IP: ");
    Serial.println(WiFi.localIP());
}

void setRGBColorBasedOnTemperature(double temperature) {
  int magentaValue = map(temperature, 0, 40, 255, 0) + 20;   // Map temperature to magenta color intensity
  int yellowValue = map(temperature, 0, 40, 255, 0)+ 20;    // Map temperature to yellow color intensity
  int cyanValue = map(temperature, 0, 40, 0, 255);      // Map temperature to cyan color intensity

  analogWrite(magentaPin, constrain(magentaValue, 0, 255)); // Ensure the value is within the valid range
  analogWrite(yellowPin, constrain(yellowValue, 0, 255));   // Ensure the value is within the valid range
  analogWrite(cyanPin, constrain(cyanValue, 0, 255));      // Ensure the value is within the valid range
}

void printSensorValue(int value) {
  Serial.println(value);
}

void adjustLEDBrightness(int sensorValue) {
  // Map the sensor value to the LED brightness range (0-255)
  int brightness = map(sensorValue, 0, 4095, 255, 0);  // Invert the mapping
  analogWrite(ledPin, brightness);
}

void displaySensorValueOnScreen(int value) {
  tft.fillScreen(TFT_BLACK); // Clear the screen

  tft.setTextSize(2);
  tft.setTextColor(TFT_WHITE);
  tft.setCursor(10, 10);
  tft.print("Photocell Value:");

  tft.setTextSize(3);
  tft.setTextColor(TFT_YELLOW);
  tft.setCursor(10, 35);
  tft.print(value);
}

void displayTemperatureOnScreen(double temperature) {
  tft.setTextSize(2);
  tft.setTextColor(TFT_WHITE);
  tft.setCursor(10, 80);
  tft.print("Temperature: ");
  tft.setTextSize(3);
  tft.setTextColor(TFT_YELLOW);
  tft.setCursor(10, 105);
  tft.print( String(temperature) + " °C");
}

void enableNightMode() {
  tft.invertDisplay(true); // Invert colors for night mode
}

void disableNightMode() {
  tft.invertDisplay(false); // Restore normal colors for day mode
}

double readRawSensorData() {
  double adc = analogRead(ThermistorPin);
  adc = ADC_LUT[(int)adc];
  return adc;
}

double readTemperature() {
  double Vout, Rt = 0;
  double T, Tc, Tf = 0;

  double adc = readRawSensorData();

  Vout = adc * Vs / adcMax;
  Rt = R1 * Vout / (Vs - Vout);

  T = 1 / (1 / To + log(Rt / Ro) / Beta);    // Temperature in Kelvin
  Tc = T - 273.15;                           // Celsius
  Tf = Tc * 9 / 5 + 32;                      // Fahrenheit

  return Tc;
}
