void setup() {
  Serial.begin(115200);
  connectToWiFi();
  tft.init();
  tft.setRotation(1); // Adjust the rotation if needed

  pinMode(ledPin, OUTPUT);
  pinMode(magentaPin, OUTPUT);
  pinMode(yellowPin, OUTPUT);
  pinMode(cyanPin, OUTPUT);
}

void loop() {
  int sensorValue = analogRead(photoCellPin);
  printSensorValue(sensorValue);
  adjustLEDBrightness(sensorValue);
  displaySensorValueOnScreen(sensorValue);

  // Check if it's dark (night mode)
  if (sensorValue < nightModeThreshold) {
    enableNightMode();
  } else {
    disableNightMode();
  }

  double temperatureCelsius = readTemperature();
  double rawSensorData = readRawSensorData();

  if (temperatureCelsius > 0) {
    Serial.println("Temperature (C): " + String(temperatureCelsius));
    displayTemperatureOnScreen(temperatureCelsius);
    setRGBColorBasedOnTemperature(temperatureCelsius);
  }

  Serial.print("Raw Sensor Data: ");
  Serial.println(rawSensorData);

  delay(1000); // Add a small delay for stability
}
